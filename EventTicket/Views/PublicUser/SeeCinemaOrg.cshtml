@using EventTicket.App_Code;
@using System.Data;
@{
    ViewBag.Title = "SeeCinemaOrg";
    Layout = "~/Views/Shared/PublicUser.cshtml";
    Base b = new Base();
    DataTable dt = b.getAllByQuery("select distinct EOrgID from Event where ECategoryID =" + 19);
}
<div class="contentHeadBox">
    @{
        foreach (DataRow row in dt.Rows)
        {
            int EOrgID = Convert.ToInt32(row["EOrgID"]);
            string Name = b.getStringByQuery("select * from EOrg where ID=" + EOrgID, "Name");
            <a href="~/PublicUser/Event?EOrgID=@EOrgID&ECategoryID=19"><img src="~/Image/open.png" />&nbsp;&nbsp;&nbsp;&nbsp;<b>@Name</b></a><br />
        }
    }
</div>

